<%- include('../partials/header') %>

<main>
    <div id="pFileHead">
        <h1><%= user.name %></h1>
        <img id="avImg" src="<%= user.avatar %>" alt="User Picture">
        <p><%= user.born %></p>
    </div><br>
    <div id="pFileBody">
        <h3>Roles: </h3>
        <table>
            <thead>
                <tr>
                    <th>Title: </th>
                    <th>Role: </th>
                </tr>
            </thead>
            <tbody>
                <% production.find(function(u){ %>
                    <tr>
                        <td><%= u.title %></td>
                        <td><%= u. %></td>
                    </tr>
                <% }) %>
            </tbody><br>
            <input type="submit" name="Add a Role">
        </table>
    </div>
</main>

<%- include('../partials/footer') %>